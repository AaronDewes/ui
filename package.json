    "@vueuse/core": "^10.9.0",
    "defu": "^6.1.4",
    "ohash": "^1.1.3",
    "nuxt-icon": "^0.6.9",
    "radix-vue": "^1.5.2",
    "tailwind-variants": "^0.2.0",
    "tailwindcss": "4.0.0-alpha.8"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "^3.11.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "happy-dom": "^13.6.2",
    "nuxt": "npm:nuxt-nightly@latest",
    "nuxt-ui-dev-module": "workspace:*",
    "vitest": "^1.3.1",
    "vitest-environment-nuxt": "^1.0.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.6"
  },
  "resolutions": {
    "nuxt-ui3": "workspace:*"
  }
